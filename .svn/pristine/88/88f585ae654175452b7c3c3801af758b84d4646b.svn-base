#import "DLBottomTitleButton.h"

@implementation DLBottomTitleButton

- (void)layoutSubviews
{
    [super layoutSubviews];
    self.imageView.y=10;
    self.imageView.centerX=self.width/2.0;
    self.titleLabel.y=self.width;
    self.titleLabel.centerX=self.width/2.0;
}
@end
